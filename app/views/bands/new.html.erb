<% form_for @band do |f| %>
  <%= f.label :bandname %>
  <%= f.text_field :name %>
  <%= f.label :username %>
  <%= f.text_field :user_name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :profile %>
  <%= f.text_field :profile %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit %>
<% end %>
