<%= form_for @musician do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :instrument %>
  <%= f.text_field :instrument %>
  <%= f.label :profile %>
  <%= f.text_field :profile %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit %>
<% end %>
