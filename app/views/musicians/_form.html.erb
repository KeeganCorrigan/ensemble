<div class="all-forms">
  <%= form_for @musician, :html => { :class => 'base-forms' } do |f| %>
      <%= f.label :name, class: "cool-label" %>
    <div class="form-group">
      <%= f.text_field :name, class: "form-width" %>
    </div>
      <%= f.label :username, class: "cool-label" %>
    <div class="form-group">
      <%= f.text_field :username, class: "form-width" %>
    </div>
      <%= f.label :instrument, class: "cool-label" %>
    <div class="form-group">
      <%= f.text_field :instrument, class: "form-width" %>
    </div>
      <%= f.label :profile, class: "cool-label" %>
    <div class="form-group">
      <%= f.text_field :profile, class: "form-width" %>
    </div>
      <%= f.label :password, class: "cool-label" %>
    <div class="form-group">
      <%= f.password_field :password, class: "form-width" %>
    </div>
    <div class="form-group">
      <%= f.submit "Start making music", class:"submit-button btn large-button" %>
    </div>
  </div>
</div>
<% end %>
